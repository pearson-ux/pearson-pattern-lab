                    </aside>
        </div><!--white-pane-->
      </div><!--sub-container-->
    </div>

    {{#oldpattern}}
    {{#subheader}}
    <div class="subheader-container assignment-list">
        <div class="subheader">
        <div class="breadcrumbs">
            <div>
                <ul>
                    <li><a href="#">{{subheader.listone}}</a></li>
                    <li class="crumb"><svg aria-hidden="true" focusable="false" class="pe-icon--chevron-next-18"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#chevron-next-18"></use></svg></li>
                    <li>{{subheader.listthree}}</li>
                </ul>
            </div>
        </div>
    </div>
    </div>
    {{/subheader}}

    <div class="container student-view">
        <div class="flex {{styleModifier}}">
            <h1 class="left-col">{{assignmentTitle}}</h1>
            {{#closeBtn}}<div class="right-col text-center">{{>atoms-xlarge-btn}}</div>{{/closeBtn}}
        </div>
        <div class="sub-container">
            <div class="flex padding-bottom mobile-column">
                <div class="left-col">
                    {{> molecules-student-assignment}}
                </div>
                <aside class="right-col background">
                    {{#scoreBox}}{{/scoreBox}}
                    {{> molecules-student-assignment-actions }}
                    {{> molecules-student-rubric }}
                </aside>
            </div>
        </div>
