{{#toggle}}
<div class="toggle">
    <button role="switch" name="{{id}}" value="false" id="{{id}}" class="pe-toggle" aria-invalid="false" aria-labelledby="{{labelId}}" aria-checked="false">
        <span>{{onIndicator}}</span>
        <span>{{offIndicator}}</span>
    </button>
    <label for="{{id}}" id="{{labelId}}" class="pe-toggle">{{label}}</label>
</div>
{{/toggle}}
